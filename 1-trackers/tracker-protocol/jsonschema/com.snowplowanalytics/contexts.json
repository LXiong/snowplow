{
	"$schema": "http://json-schema.org/schema#",
	"description": "Schema for custom contexts",
	"type": "array",

	"items": {

		"type": "object",

		"properties": {

			"header": {
				"type": "object",
				"properties": {
					"vendor": {
						"type": "string"
					},
					"type": {
						"type": "string"
					},
					"version": {
						"type": "string"
					}
				},
				"required": ["vendor", "type", "version"]
			},

			"data": {
				"type": "object"
			}
		},

		"required": ["header", "data"]
	},

	"minItems": 1
}
