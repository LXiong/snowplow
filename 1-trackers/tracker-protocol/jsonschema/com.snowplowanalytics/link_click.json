{
	"$schema": "http://json-schema.org/schema#",
	"description": "Schema for a link click event",
	"self": {
		"vendor": "com.snowplowanalytics",
		"name": "link_click",
		"format": "json",
		"version": "1-0-0"
	},

	"type": "object",
	"properties": {
		"elementId": {
			"type": "string"
		},
		"elementClasses": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"elementTarget": {
			"type": "string"
		},
		"targetUrl": {
			"type": "string",
			"minLength": 1
		}
	},
	"required": ["targetUrl"],
	"additionalProperties": false
}
