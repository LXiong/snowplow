{
	"$schema": "http://json-schema.org/schema#",
	"title": "com.snowplowanalytics/link_click/1.0.0",
	"description": "Schema for a link click event",
	"type": "object",
	"properties": {
		"elementId": {
			"type": "string"
		},
		"elementClasses": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"elementTarget": {
			"type": "string"
		},
		"targetUrl": {
			"type": "string",
			"minLength": 1
		}
	},
	"required": ["targetUrl"],
	"additionalProperties": false
}
