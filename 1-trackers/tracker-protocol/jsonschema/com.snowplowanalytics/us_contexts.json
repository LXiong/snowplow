{
	"$schema": "http://json-schema.org/schema#",
	"description": "Schema for an envelope of custom contexts",
	"type": "array",
	"items": {

		"type": "object",

		"properties": {
			"vendor": {
				"type": "string"
			},
			"name": {
				"type": "string"
			},
			"version": {
				"type": "string"
			},
			"properties": {
				"type": "object",
				"minProperties": 1
			}
		},

		"required": ["vendor", "name", "version", "properties"]

	},

	"minItems": 1
}
